package correct;

import correct.example.sd;

sequencediagram allGrammarElements {

	// Interacting objects
	<<someStereotype>> (C) o: Order;
	c: Customer;
	
	// Offer -> Production
	o -> c : <<trigger>> sendConfirmation();
	o <-- c : return;
	o -> o : orderParts();
	<o: [state == Production]>
	
	// Production -> Shipping
	{{"JAVA: Make sure that allItemsProduced() is true"}}
	o -> o : <<trigger>> shipItems();
	<o : [state == Shipping]>
	
	// Shipping -> Payment
	{{"JAVA :  Make sure that allItemsSent() is true"}}
	o -> c : sendInvoice(sum);
	c --> o : return;
	<o: [state == payment]>
	
	// Payment -> Complete
	{{"Java:  Make sure that paid is true"}}
	<o: [state == complete]>
	o -> (m : Mail) : new Mail();
	
}