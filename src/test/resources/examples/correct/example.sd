package examples.correct;

sequencediagram example {

  // Interacting objects
  o: Order;
  c: Customer;

  // Offer -> Production
  o -> c : trigger sendConfirmation();
  o <- c : return;
  o -> o : orderParts();
  assert state == Production;

  // Production -> Shipping
  // java: {produceAllItems()}
  o -> o : trigger shipItems();
  assert state == Shipping;

  // Shipping -> Payment
  // java: {sendAllItems()}
  o -> c : sendInvoice(sum);
  o <- c : return;
  assert state == payment;

  // Payment -> Complete
  // java: {pay()}
  assert state == complete;

}
