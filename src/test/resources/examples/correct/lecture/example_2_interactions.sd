package examples.correct.lecture;

sequencediagram example_2_interactions {

  // Interacting objects
  a : A;
  b : B;

  // Method calls
  a -> b : methodenname();
  a -> b : methodenname(arg);
  a -> b : methodenname(arg1, arg2);
  a -> b : methodenname;
  a -> b : methodenname(...);

  // Returns
  a <- b : return Ergebnis;
  a <- b : return ...;
  a <- b : return;
  a <- b :;

  // Exceptions
  a <- b : throw ExceptionTyp();
  a <- b : throw ExceptionTyp(arg);
  a <- b : throw ExceptionTyp(arg1, arg2);
  a <- b : throw ExceptionTyp;
  a <- b : throw ExceptionTyp(...);
}
