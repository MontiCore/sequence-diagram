package examples.correct.lecture;

sequencediagram example_2_interactions {

  // Interacting objects
  a : A;
  b : B;

  // Method calls
  a -> b : methodenname()
  a -> b : methodenname(arg)
  a -> b : methodenname(arg1, arg2)
  a -> b : methodenname
  a -> b : methodenname(...)

  // Returns
  b <-- a : return Ergebnis
  b <-- a : return ...
  b <-- a : return
  b <-- a :

  // Exceptions
  b <-- a : throw ExceptionTyp()
  b <-- a : throw ExceptionTyp(arg)
  b <-- a : throw ExceptionTyp(arg1, arg2)
  b <-- a : throw ExceptionTyp
  b <-- a : throw ExceptionTyp(...)

}
