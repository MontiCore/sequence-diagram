sequencediagram imcomplete_not_allowed {

	... a : A;
	(C) b : B;
	<<match:visible>> c : C;
	<<match:initial>> d : D;
	
	a ->  a : method(...); 		// OK
	a ->  a : new Object(...);	// OK
	a --> a : Exception(...);	// OK
	a --> a : return ...;		// OK	
		
	b ->  a : method(...);		// Not OK
	b ->  a : new Object(...);	// Not OK
	b --> a : Exception(...);	// Not OK
	b --> a : return ...;		// Not OK
	
	c ->  a : method(...); 		// OK
	c ->  a : new Object(...);	// OK
	c --> a : Exception(...);	// OK
	c --> a : return ...;		// OK	
	
	d ->  a : method(...); 		// OK
	d ->  a : new Object(...);	// OK
	d --> a : Exception(...);	// OK
	d --> a : return ...;		// OK	
	

}