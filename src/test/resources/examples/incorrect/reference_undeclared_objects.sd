package examples.incorrect;

sequencediagram example_2_interactions {

  // Declarations
  a : A;
  b : B;
  c : C;

  // References
  a -> b : methodenname();
  a -> d : methodenname();  //invalid
  e -> a : methodenname();  //invalid
  a -> (e:E) : methodenname();
  a -> e : methodenname();  //valid!
  d --> a : return;       //invalid
  d <-- a : return;       //invalid
  d --> a : Exception();    //invalid
  a --> d : Exception();    //invalid


}
